{
  "name": "msswebsite",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.1.0",
    "tailwindcss": "^2.1.2"
  },
  "scripts": {
    "css:serve": "postcss src/css/styles.pcss --o src/css/_site/styles.css --watch",
    "eleventy:serve": "eleventy --serve",
    "serve": "concurrently \"yarn css:serve\" \"yarn serve\"",
	"css:build": "postcss src/css/styles.pcss --o src/css/_site/styles.css",
    "build": "yarn css:build && yarn eleventy"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "fast-glob": "^3.2.5",
    "postcss-cli": "^8.3.1"
  }
}
